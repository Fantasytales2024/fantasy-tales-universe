{
  "name": "fantasy-tales-universe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "json-server --watch src/data/db.json --port 3001",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "process-images": "node scripts/process-images.js",
    "verify-images": "node scripts/verify-images.js",
    "optimize-images": "node scripts/optimize-images.js",
    "convert-icons": "node scripts/convert-icons.js",
    "images:all": "pnpm process-images && pnpm optimize-images && pnpm verify-images"
  },
  "dependencies": {
    "firebase": "^11.3.1",
    "framer-motion": "^11.0.3",
    "maath": "^0.10.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@heroicons/react": "^2.0.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/potrace": "^2.1.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.0.0",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.4.31",
    "postcss-nesting": "^13.0.1",
    "potrace": "^2.1.8",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.0.6"
  }
}